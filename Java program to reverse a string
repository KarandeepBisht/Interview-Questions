public class Main
{
    static void swap(char[] a, int index1, int index2){
    char temp=a[index1];
    a[index1]=a[index2];
    a[index2]=temp;}
    static void reverse(char[] str){
         int n = str.length;
        for(int i=0; i<n/2; i++){
            swap(str, i, n - i - 1);
        }
    }
    
	public static void main(String[] args) {
	    String str="GeeksQuiz";
	    char[] s=str.toCharArray();
	    reverse(s);
	    System.out.println(s);
	}
}
